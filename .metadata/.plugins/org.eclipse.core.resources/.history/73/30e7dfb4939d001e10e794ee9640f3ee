package com.cc.feign;

import org.springframework.cloud.openfeign.FeignClient;
import org.springframework.http.ResponseEntity;
import org.springframework.stereotype.Service;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PathVariable;
import org.springframework.web.bind.annotation.RequestParam;

import com.cc.utilityHelper.GeneralResponse;

@FeignClient("BANK-SERVICE")
public interface BankExternalServices {
	
	@GetMapping("/getAllBanksByUserId/{userId}")
	public  ResponseEntity<GeneralResponse> getAllBanksByUserId(@PathVariable Integer userId) ;
	
	

}
